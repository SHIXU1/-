<view class='web' wx:for="{{list}}" wx:key="{{index}}">
    <view class='top'>
        <image src='{{item.pic}}' class='img'></image>
    </view>
    <view>{{item.title}}</view>
     <view>{{item.author}}</view>
     <audio src='{{item.url}}' controls="true" poster="{{item.pic}}" name="{{item.title}}" author="{{item.author}}"></audio>
</view>