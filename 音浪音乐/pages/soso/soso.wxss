page{
  width: 100%;
  height: 100%;
}
.web{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.top{
  width: 100%;
  height: 90rpx;
  display: flex;
  align-items: center;
}
.int{
  flex: 1;
  height: 60rpx;
  margin-left: 40rpx;
  margin-right:20rpx;
  border: 1px solid #ccc;
  border-radius: 25rpx;
  
}
.qu{
  width:120rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}
.box{
  width: 100%;
  flex: 1;
}
.boxtop{
  margin-left: 40rpx;
}
.p{
  display: flex;
  flex-wrap: wrap;
}
.span{
  padding: 10rpx 20rpx;
  background: #ccc;
  margin: 20rpx 20rpx;
  border-radius: 25rpx;
}
.boxleft{
  display: none;
}
.name{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #ccc;
  display: flex;
}
.title{
  background: linear-gradient(to right, #702eff, #bd2eff);
  -webkit-background-clip: text;
  color: transparent;
}
