page{
  width: 100%;
  height: 100%;
}
.web{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.top{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.int{
  flex: 1;
  height: 80rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  border-radius: 25rpx;
  border: 1px solid #ccc;
}
.span{
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-right: 30rpx;
}
.box{
  width: 100%;
  flex: 1;
  overflow-y: auto;
}
.title{
  width: 100%;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  font-weight: bold;
  font-size: 40rpx;
}
.name{
  width: 100%;
  text-align: center;
}
.text{
  width: 80%;
  margin-left: 10%;
  text-align: center;
}
.aa{
  display: none;
}